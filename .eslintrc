{
  {
  "extends": "airbnb",
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "import/no-extraneous-dependencies": 0,
    "max-len": ["warn", 120],
    "class-methods-use-this": [0],
    "no-unused-vars": 0,
    "arrow-body-style": 0,
  }
}
